{
    "autoload": {
        "psr-4": {
            "Bisarca\\RobotsTxt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bisarca\\RobotsTxt\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Bisarca robots.txt Management",
    "license": "MIT",
    "name": "bisarca/robots-txt",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "^5.4",
        "symfony/phpunit-bridge": "^3.1"
    },
    "scripts": {
        "test": [
            "php-cs-fixer fix -vv --ansi",
            "phpunit --colors=always"
        ]
    }
}

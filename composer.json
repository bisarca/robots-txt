{
    "autoload": {
        "psr-4": {
            "Bisarca\\RobotsTxt\\": "src/Bisarca/RobotsTxt/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bisarca\\RobotsTxt\\": "tests/Bisarca/RobotsTxt/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Robots.txt Component",
    "license": "MIT",
    "name": "bisarca/robots-txt",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.12",
        "jeremykendall/php-domain-parser": "^3.0",
        "phpunit/phpunit": "^5.5",
        "symfony/phpunit-bridge": "^3.1"
    },
    "scripts": {
        "test": [
            "php-cs-fixer fix -vv --ansi",
            "phpunit --colors=always"
        ]
    },
    "suggest": {
        "jeremykendall/php-domain-parser": "for advanced validation of the Host directives"
    }
}
